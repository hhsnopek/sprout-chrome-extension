{
    "manifest_version": 2,
    "version": "1.0.0",

    "name": "<%= name %>",
    "description": "<%= description %>",
    "homepage_url": "//github.com/<%= github_username %>/<%= name %>",
    "author": "<%= github_username %> (//github.com/<%= github_username %>)",

	  "content_scripts": [ {
      "matches": [""],
      "js": ["src/js/jquery.min.js", "src/js/index.js"],
      "css": ["src/css/index.css"],
      "run_at": "document_idle"
    } ]
}
